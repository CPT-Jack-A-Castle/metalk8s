  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start:standalone": "NODE_ENV=development webpack-dev-server --config ./config/webpack.config.micro.dev.js --port 3001 --env=development",
    "build": "webpack --config ./config/webpack.config.micro.prod.js -p",
    "test:e2e": "cypress run --spec 'cypress/integration/**/*.feature'",
    "flow": "flow check"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.8.4",
    "@redux-saga/testing-utils": "^1.0.2",
    "compression-webpack-plugin": "^3.0.0",
    "customize-cra": "^0.4.1",
    "cypress": "^3.2.0",
    "cypress-cucumber-preprocessor": "1.12.0",
    "cypress-wait-until": "1.6.0",
    "flow-bin": "^0.107.0",
    "jest-junit": "^7.0.0",
    "react-app-rewired": "^2.1.3",
    "source-map-explorer": "^2.0.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-external-import": "github:carlito-scality/webpack-external-import.git#microapp-poc"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "jest-junit": {
    "output": "junit/jest-junit.xml",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleNameTemplate": "{classname} {title}"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "interleave": {
    "src/containers/MicroApp": "MetalMicroApp",
    "src/containers/Toto": "Toto"
  }
}
